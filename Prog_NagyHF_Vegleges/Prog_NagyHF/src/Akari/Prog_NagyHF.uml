          <ownedParameter xmi:id="_kAVtJW0cEe2CGbEexmvy_w" type="_j_ZR8G0cEe2CGbEexmvy_w" direction="return">
            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>int r=getRowSize(m);&#xD;
		int c=getColSize(m);&#xD;
		ArrayList&lt;ArrayList&lt;Character>> lines =new ArrayList&lt;>(r);&#xD;
		try {&#xD;
	   		BufferedReader br=new BufferedReader(new FileReader(t+m+&quot;.txt&quot;)); &#xD;
			String line;&#xD;
			int i=0;	&#xD;
			while ((line=br.readLine()) != null) &#xD;
			{&#xD;
				lines.add(new ArrayList&lt;Character>(c));&#xD;
				for(int j=0; j&lt;c; j++) {&#xD;
					lines.get(i).add(line.charAt(j));&#xD;
				}&#xD;
				i++;&#xD;
		    }&#xD;
		br.close();&#xD;
		} catch (IOException e) {&#xD;
			System.out.println(&quot;File not found!&quot;);&#xD;
			e.printStackTrace();&#xD;
		}&#xD;
		return lines;&#xD;
	</body>
